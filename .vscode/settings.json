{
    // The following variables require setting for the custom ROS2 Launch and Task commands to run
    // Ensure you are opening your workspace folder, not your package folder, as a project (e.g. ros2_ws)
    "ros.distro": "humble",
    "ros2_dir": "/opt/ros/${config:ros.distro}",
    "username": "napier",
    "package_name": "slamtec_rplidar_ros2",
    "node_name": "rplidar_scan_publisher",
    "DCMAKE_BUILD_TYPE": "Debug",
    "run_command": "ros2 run --prefix \"gdbserver localhost:3000\" ${config:package_name} ${config:node_name}",
    "launch_command": "ros2 launch --launch-prefix \"gdbserver localhost:3000\" ${config:package_name} view_rplidar_launch.py",
    
    "cmake.sourceDirectory": "${workspaceFolder}/src/${config:package_name}",
    "python.autoComplete.extraPaths": [
        "/opt/ros/${config:ros2.distro}/lib/python3.10/site-packages",
        "/opt/ros/${config:ros2.distro}/local/lib/python3.10/dist-packages"
    ],

