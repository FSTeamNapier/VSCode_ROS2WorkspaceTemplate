{
    "configurations": [
        {
            "browse": {
                "databaseFilename": "${default}",
                "limitSymbolsToIncludedHeaders": false
            },
            "includePath": [
                "${config:workspace_dir}/install/${config:package_name}/include/**",
                "${config:workspace_dir}/src/${config:package_name}/include/**",
                "${workspaceFolder}/include/**",
                "${config:workspace_dir}/src/${config:package_name}/sdk/include/**",
                "${config:workspace_dir}/src/${config:package_name}/sdk/src",
                "${workspaceFolder}/sdk/include/**",
                "${workspaceFolder}/sdk/src",

                "${config:ros2_dir}/include/**",
                "/usr/include/**",
            ],
            "name": "ROS",
            "intelliSenseMode": "gcc-x64",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu11",
            "cppStandard": "c++14"
        }
    ],
    "version": 4
}
